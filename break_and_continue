// *** break and continue ***

for (k=1; k<=6; k++)
{
    if (k == 4)
    {
        break;   //breaks the loop
    }
    console.log(k);
}
console.log("hello there");


for (k=1; k<=6; k++)
{
    if (k == 4)
    {
        continue;   //skips the current iteration
    }
    console.log(k);
}
console.log("Now there");
